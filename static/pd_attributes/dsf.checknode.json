[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "text"
      },
      {
        "text": "数据配置",
        "bindControlAttr": "slots",
        "changeExecFunction": "renderSlots",
        "showType": "dialog",
        "config": {
          "content": "TreeSelectorEdit",
          "width": "1400px",
          "height": "60vh",
          "params": {
            "type": "checkNode"
          }
        }
      },
      {
        "text": "是否呈现标签样式",
        "bindControlAttr": "showTab",
        "showType": "boolean",
        "hidden": "@[slots].length==1"
      }
    ]
  },
  {
    "name": "选择后区域",
    "attributes": [
      {
        "text": "数量上限(小于1则为无上限)",
        "bindControlAttr": "maxLength",
        "showType": "number",
        "changeExecFunction": "renderMaxLength"
      },
      {
        "text": "是否可排序",
        "bindControlAttr": "sort",
        "changeExecFunction": "renderSort",
        "showType": "boolean"
      },
      {
        "text": "选择值展现样式",
        "bindControlAttr": "typeStyle",
        "showType": "select",
        "hidden": "@[slots].length==1",
        "dictionary": [
          {
            "text": "列表式",
            "value": "list"
          },
          {
            "text": "树结构",
            "value": "tree"
          }
        ]
      }
    ]
  }
]