[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "text"
      },
      {
        "text": "名称",
        "bindControlAttr": "label",
        "showType": "text",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "是否可见",
        "showType": "boolean",
        "bindControlAttr": "visible",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "是否只读",
        "showType": "boolean",
        "bindControlAttr": "readOnly",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "照片/按钮模式切换",
        "showType": "boolean",
        "bindControlAttr": "switchMode",
        "changeExecFunction": "renderContext",
        "dictionary": [
          {
            "value": true,
            "text": "照片模式"
          },
          {
            "value": false,
            "text": "按钮模式"
          }
        ]
      }
    ]
  },
  {
    "name": "图片属性",
    "attributes": [
      {
        "text": "是否显示图片上传限制提示",
        "bindControlAttr": "isShowTips",
        "showType": "boolean",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "是否裁剪照片",
        "showType": "boolean",
        "bindControlAttr": "isCutPic",
        "hiddenKeyClass": "cutRatio",
        "showKeyClass": "cutRatio",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "剪裁比例(如：16/9)",
        "showType": "text",
        "bindControlAttr": "aspectRatio",
        "changeExecFunction": "renderContext",
        "class": "cutRatio",
        "hidden": "@[isCutPic]==true"
      },
      {
        "text": "照片宽度(px)",
        "bindControlAttr": "imgWidth",
        "showType": "text",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "照片高度(px)",
        "bindControlAttr": "imgHeight",
        "showType": "text",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "上传图片大小限制",
        "bindControlAttr": "maxFileSize",
        "showType": "text",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "默认图片",
        "bindControlAttr": "defaultImg",
        "showType": "select",
        "dictionary": [
          {
            "value": "0",
            "text": "头像"
          },
          {
            "value": "1",
            "text": "封面"
          }
        ],
        "changeExecFunction": "renderContext"
      },
      {
        "text": "是否去掉图片外边距",
        "bindControlAttr": "hasPadding",
        "showType": "select",
        "dictionary": [
          {
            "value": "0",
            "text": "是"
          },
          {
            "value": "1",
            "text": "否"
          }
        ],
        "changeExecFunction": "renderContext"
      },
      {
        "text": "是否去掉边框",
        "bindControlAttr": "hasBorder",
        "showType": "select",
        "dictionary": [
          {
            "value": "0",
            "text": "是"
          },
          {
            "value": "1",
            "text": "否"
          }
        ],
        "changeExecFunction": "renderContext"
      },
      {
        "text": "文件上传接口",
        "bindControlAttr": "requestUrl",
        "showType": "text",
        "changeExecFunction": "renderContext"
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "元数据",
        "bindControlAttr": "metadata.valueAttributes",
        "showType": "dialog",
        "config": {
          "content": "DesValueAttributes",
          "width": "600px",
          "height": "60vh",
          "buttonText": "设置",
          "title": "设置"
        }
      },
      {
        "text": "二次开发配置",
        "bindControlAttr": "development",
        "showType": "dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh"
        }
      },
      {
        "text": "必填标识",
        "bindControlAttr": "required",
        "showType": "boolean",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "校验规则",
        "bindControlAttr": "metadata.validate",
        "showType": "dialog",
        "config": {
          "content": "DesValidateRule",
          "width": "800px",
          "height": "60vh",
          "buttonText": "设置",
          "title": "设置"
        }
      }
    ]
  }
]