[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "text"
      },
      {
        "text": "名称",
        "bindControlAttr": "label",
        "showType": "text",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "名称是否显示",
        "bindControlAttr": "showLabel",
        "showType": "boolean",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "是否可见",
        "showType": "boolean",
        "bindControlAttr": "visible",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "是否只读",
        "showType": "boolean",
        "bindControlAttr": "readOnly",
        "changeExecFunction": "renderContext"
      }
    ]
  },
  {
    "name": "控件属性",
    "attributes": [
      {
        "text": "是否裁剪照片",
        "showType": "boolean",
        "bindControlAttr": "isCutPic",
        "hiddenKeyClass": "cutRatio",
        "showKeyClass": "cutRatio",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "剪裁比例(如：16/9)",
        "showType": "text",
        "bindControlAttr": "aspectRatio",
        "class": "cutRatio",
        "changeExecFunction": "renderContext",
        "hidden": "@[isCutPic]==true"
      },
      {
        "text": "按钮名称是否显示",
        "bindControlAttr": "showBtnName",
        "showType": "boolean",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "是否支持多张上传",
        "bindControlAttr": "multiple",
        "showType": "boolean",
        "hiddenKeyClass": "fileNum",
        "showKeyClass": "fileNum",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "只读状态下格式化",
        "showType": "select",
        "bindControlAttr": "formatter",
        "desc": "<p>只读状态下格式化<p>",
        "dictionary": [
          {
            "text": "显示名称",
            "value": "multipleFiles^originalFileName"
          }
        ]
      },
      {
        "text": "上传图片数量",
        "bindControlAttr": "uploadFileNum",
        "showType": "text",
        "class": "fileNum",
        "changeExecFunction": "renderContext",
        "hidden": "@[multiple]==true"
      },
      {
        "text": "上传图片大小限制",
        "bindControlAttr": "maxFileSize",
        "showType": "text",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "允许上传图片后缀名(','分隔)",
        "bindControlAttr": "uploadFileExts",
        "showType": "text"
      },
      {
        "text": "文件打包下载文件名称",
        "bindControlAttr": "zipFileName",
        "showType": "text"
      },
      {
        "text": "文件上传接口",
        "bindControlAttr": "requestUrl",
        "showType": "text",
        "changeExecFunction": "renderContext"
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "元数据",
        "bindControlAttr": "metadata.valueAttributes",
        "showType": "dialog",
        "config": {
          "content": "DesValueAttributes",
          "width": "600px",
          "height": "60vh",
          "buttonText": "设置",
          "title": "设置"
        }
      },
      {
        "text": "二次开发配置",
        "bindControlAttr": "development",
        "showType": "dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh"
        }
      },
      {
        "text": "描述文字",
        "showType": "text",
        "bindControlAttr": "dataCapion",
        "desc": "<p>移动端运行该组件时会在名称后追加一段备注文字。</p><p>因布局上的问题PC端暂不支持</p>"
      },
      {
        "text": "必填标识",
        "bindControlAttr": "required",
        "showType": "boolean",
        "changeExecFunction": "renderContext"
      },
      {
        "text": "校验规则",
        "bindControlAttr": "metadata.validate",
        "showType": "dialog",
        "config": {
          "content": "DesValidateRule",
          "width": "800px",
          "height": "60vh",
          "buttonText": "设置",
          "title": "设置"
        }
      }
    ]
  }
]